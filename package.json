{
  "private": true,
  "packageManager": "pnpm@7.3.0",
  "workspaces": [
    "packages/*",
    "play",
    "docs"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "update:version": "tsx scripts/update-version.ts",
    "gen:version": "tsx scripts/gen-version.ts",
    "dev:play": "pnpm -C playground dev",
    "docs:dev": "pnpm run -C docs dev",
    "clean": "pnpm run clean:dist && pnpm run -r --parallel clean",
    "clean:dist": "rimraf dist",
    "typecheck:web": "vue-tsc -p tsconfig.web.json --composite false --noEmit",
    "build": "pnpm run -C internal/build start",
    "stub": "pnpm run -r --parallel stub",
    "postinstall": "pnpm stub && concurrently \"pnpm gen:version\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@antv/x6": "^2.15.6",
    "@antv/x6-plugin-scroller": "^2.0.10",
    "@antv/x6-vue-shape": "^2.1.1",
    "@element-plus/icons-vue": "^2.1.0",
    "@p-helper/build-constants": "workspace:*",
    "@p-helper/components": "workspace:*",
    "@p-helper/constants": "workspace:*",
    "@p-helper/hooks": "workspace:^",
    "@p-helper/playground": "workspace:^",
    "@p-helper/theme-chalk": "workspace:^",
    "@p-helper/utils": "workspace:*",
    "@types/lodash": "^4.14.197",
    "@vueuse/core": "^10.9.0",
    "lodash-es": "^4.17.21",
    "lodash-unified": "^1.0.3",
    "mathjs": "^11.9.1",
    "type-fest": "^4.2.0",
    "unplugin-vue-macros": "^2.4.6",
    "vue": "^3.4.21",
    "vue-types": "^5.1.1"
  },
  "devDependencies": {
    "@esbuild-kit/cjs-loader": "^2.4.2",
    "@p-helper/build": "workspace:*",
    "@p-helper/build-utils": "workspace:*",
    "@p-helper/eslint-config": "workspace:^",
    "@p-helper/types": "workspace:*",
    "@pnpm/find-workspace-packages": "^6.0.9",
    "@pnpm/logger": "^5.0.0",
    "@pnpm/types": "^9.2.0",
    "@types/gulp": "^4.0.13",
    "@types/lodash-es": "^4.17.8",
    "@types/mathjs": "^9.4.2",
    "@types/node": "^20.5.0",
    "@vitejs/plugin-vue": "^4.3.1",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "chalk": "^5.3.0",
    "concurrently": "^8.2.0",
    "consola": "^3.2.3",
    "element-plus": "^2.5.6",
    "eslint": "^8.47.0",
    "gulp-typescript": "6.0.0-alpha.1",
    "prettier": "^3.0.2",
    "rimraf": "^5.0.1",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.66.0",
    "ts-morph": "^19.0.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "unplugin-vue-define-options": "^1.3.15",
    "vite-plugin-dts": "^3.5.2",
    "vue-tsc": "^1.8.8"
  }
}
