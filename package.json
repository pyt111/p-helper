{
  "private": true,
  "packageManager": "pnpm@7.3.0",
  "workspaces": [
    "packages/*",
    "play",
    "docs"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "update:version": "tsx scripts/update-version.ts",
    "gen:version": "tsx scripts/gen-version.ts",
    "dev:play": "pnpm -C playground dev",
    "clean": "pnpm run clean:dist && pnpm run -r --parallel clean",
    "clean:dist": "rimraf dist",
    "build": "pnpm run -C internal/build start",
    "stub": "pnpm run -r --parallel stub",
    "postinstall": "pnpm stub && concurrently \"pnpm gen:version\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@p-helper/build-constants": "workspace:*",
    "@p-helper/components": "workspace:*",
    "@p-helper/constants": "workspace:*",
    "@p-helper/hooks": "workspace:^1.0.0",
    "@p-helper/playground": "workspace:^1.0.0",
    "@p-helper/theme-chalk": "workspace:^1.0.0",
    "@p-helper/utils": "workspace:*",
    "unplugin-vue-define-options": "^0.10.0",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@esbuild-kit/cjs-loader": "^2.3.2",
    "@p-helper/build": "workspace:1.0.0",
    "@p-helper/build-utils": "workspace:*",
    "@p-helper/eslint-config": "workspace:^1.0.0",
    "@pnpm/find-workspace-packages": "^4.0.19",
    "@pnpm/logger": "^4.0.0",
    "@pnpm/types": "^8.4.0",
    "@types/gulp": "^4.0.9",
    "@types/node": "*",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "chalk": "^5.0.1",
    "concurrently": "^7.3.0",
    "consola": "^2.15.3",
    "eslint": "^8.22.0",
    "gulp-typescript": "6.0.0-alpha.1",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "sass": "^1.53.0",
    "ts-morph": "^15.1.0",
    "tsx": "^3.8.2",
    "typescript": "^4.7.4",
    "vite": "^3.0.0"
  }
}
